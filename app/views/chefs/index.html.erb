<%= render 'shared/title', title: "All Chefs" %>

<div class="row">
  <div class="col-md-4 col-md-offset-4 text-center">
    <%= will_paginate %>
  </div>
</div>

<% @chefs.each do |chef| %>

    <div class="row">
      <div class="col-md-4 col-md-offset-4 text-center">
        <h2><%=chef.chefname%></h2>
        <%= link_to gravatar_for(chef, size: 200), chef_path(chef) %>
      </div>
    </div>

<% end %>